<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.complitex.osznconnection.file.service.AbstractRequestBean">
    <resultMap id="abstractRequest" type="org.complitex.osznconnection.file.entity.AbstractRequest">
        <id column="id" property="id"/>
        <result column="request_file_id" property="requestFileId"/>
        <result column="account_number" property="accountNumber"/>
        <result column="status" property="status"/>
        <result column="organization_id" property="organizationId"/>

        <collection property="warnings" ofType="org.complitex.osznconnection.file.entity.RequestWarning"
                resultMap="org.complitex.osznconnection.file.service.warning.RequestWarningBean.requestWarning"/>

    </resultMap>

</mapper>