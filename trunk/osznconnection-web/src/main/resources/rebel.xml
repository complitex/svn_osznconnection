<?xml version="1.0" encoding="UTF-8"?>
<application xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns="http://www.zeroturnaround.com"
             xsi:schemaLocation="http://www.zeroturnaround.com http://www.zeroturnaround.com/alderaan/rebel-2_0.xsd">

    <!--
        Variables used here require a system property passed to the application container, like this:
        -Dproject.root=c:/work/complitex
    -->

    <classpath>
        <dirset dir="${project.root}/complitex">
            <include name="**/target/classes"/>
        </dirset>

        <dirset dir="${project.root}/osznconnection">
            <include name="**/target/classes"/>
        </dirset>
    </classpath>

</application>
