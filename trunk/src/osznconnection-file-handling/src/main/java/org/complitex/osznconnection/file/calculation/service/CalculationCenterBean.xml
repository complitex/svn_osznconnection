<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.complitex.osznconnection.file.calculation.service.CalculationCenterBean">
    <resultMap id="calculationCenterPreference" type="org.complitex.osznconnection.file.entity.CalculationCenterPreference">
        <id column="calculation_center_id" property="calculationCenterId"/>
        <result column="adapter_class" property="adapterClass"/>
    </resultMap>

    <select id="getCurrentCenterInfo" resultMap="calculationCenterPreference">
        SELECT * FROM `calculation_center_preference` LIMIT 1
    </select>

</mapper>